<!DOCTYPE html>
<html>
  <head>
    <% include ../partials/header.ejs %>
  </head>
  <body>
    <header>
      <a class="header-logo" href="/" >LIST</a>
    </header>
    <h1><%=dataText%></h1>
    <div class="container">
      <div class="container-header">
        <h1>Daftar Belanjaan</h1>
      </div>
      <div class="index-table-wrapper">
        <div class="table-head">
          <span class="id-column">ID</span>
          <span>Item</span>
        </div>
        <% const items = [
          {id: 1, name: 'kentang'},
          {id: 2, name: 'wortel'},
          {id: 3, name: 'bawang'}
        ]; %>
        <ul class="table-body">
          <% items.forEach((item) => { %>
            <li>
              <span class="id-column"><%= item.id %></span>
              <span class="name-column"><%= item.name %></span>
            </li>
          <% }); %>
        </ul>
      </div>
    </div>
  </body>
</html>